{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ES2020",
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "noImplicitAny": false
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "**/*.d.ts", // Include .d.ts files in all directories
    "expo-env.d.ts"
, "babel.config.js"  ],
  "exclude": ["**/node_modules/*"]
}
